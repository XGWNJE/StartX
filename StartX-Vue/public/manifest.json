{
  "manifest_version": 3,
  "name": "StartX",
  "version": "0.1.0",
  "description": "A modern, customizable browser start page with wallpapers, search, and bookmarks",
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "StartX"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 