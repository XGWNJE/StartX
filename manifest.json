{
  "name": "StartX",
  "description": "一款简洁美观的起始标签页，让起始页做该做的事",
  "version": "1.0.1",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "favicon",
    "geolocation"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 